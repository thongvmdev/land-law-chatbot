# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here
OLLAMA_API_KEY=

# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token

# LangSmith Configuration
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=chat-langchain-js
LANGCHAIN_PROMPT_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROMPT_API_URL=https://api.smith.langchain.com

# Weaviate Configuration
# For local Docker: http://weaviate:8080
# For cloud: https://your-weaviate-cluster.weaviate.network
WEAVIATE_URL=http://weaviate:8080
# For local Docker: weaviate:50051
# For cloud: your-weaviate-cluster.weaviate.network:443
WEAVIATE_GRPC_URL=weaviate:50051
WEAVIATE_API_KEY=your_secure_weaviate_api_key_here

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_postgres_password_here
POSTGRES_DB=langgraph_persistence

# Database Connection Strings
# For local Docker: postgresql://postgres:password@postgres:5432/langgraph_persistence
# For external: postgresql://user:password@localhost:5432/langgraph
POSTGRES_URI=postgresql://postgres:your_secure_postgres_password_here@postgres:5432/langgraph_persistence
LANGGRAPH_PERSISTENCE=postgresql://postgres:your_secure_postgres_password_here@postgres:5432/langgraph_persistence
DATABASE_URL=postgresql://postgres:your_secure_postgres_password_here@postgres:5432/langgraph_persistence
RECORD_MANAGER_DB_URL=postgresql://postgres:your_secure_postgres_password_here@postgres:5432/langgraph_persistence

# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password_here
# For local Docker: redis://:password@langgraph-redis:6379
# For external: redis://:password@localhost:6379
REDIS_URI=redis://:your_secure_redis_password_here@langgraph-redis:6379

# Model Configuration
EMBEDDING_MODEL=cloudflare/qwen3-embedding-0.6b
# EMBEDDING_MODEL=ollama/nomic-embed-text  # Alternative: local Ollama
OLLAMA_BASE_URL=http://localhost:11434

# Server Configuration (for self-hosted)
PORT=3001
NODE_ENV=development

# Parser Service Configuration
PARSER_SERVICE_URL=http://localhost:8001

# Google Embeddings Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_EMBEDDING_BATCH_SIZE=5
GOOGLE_EMBEDDING_MAX_CONCURRENCY=1
GOOGLE_EMBEDDING_BATCH_DELAY=1000

# Weaviate Batch Configuration
WEAVIATE_BATCH_SIZE=50
FORCE_UPDATE=false

